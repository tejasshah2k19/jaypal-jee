<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	<display-name>jaypal-jee</display-name>
	<welcome-file-list>
		<welcome-file>Signup.html</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>SignupServlet</servlet-name>
		<servlet-class>com.google.controller.SignupServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SignupServlet</servlet-name>
		<url-pattern>/SignupServlet</url-pattern>
	</servlet-mapping>
	<filter>
		<filter-name>RegistrationFilter</filter-name>
		<filter-class>com.google.filter.RegistrationFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>RegistrationFilter</filter-name>
		<url-pattern>/RegistrationServlet</url-pattern>
	</filter-mapping>
	<servlet>
		<description></description>
		<display-name>ListCookieServlet</display-name>
		<servlet-name>ListCookieServlet</servlet-name>
		<servlet-class>com.google.controller.ListCookieServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ListCookieServlet</servlet-name>
		<url-pattern>/ListCookieServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>AddCookieServlet</display-name>
		<servlet-name>AddCookieServlet</servlet-name>
		<servlet-class>com.google.controller.AddCookieServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AddCookieServlet</servlet-name>
		<url-pattern>/AddCookieServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>DeleteCookieServlet</display-name>
		<servlet-name>DeleteCookieServlet</servlet-name>
		<servlet-class>com.google.controller.DeleteCookieServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DeleteCookieServlet</servlet-name>
		<url-pattern>/DeleteCookieServlet</url-pattern>
	</servlet-mapping>

	<session-config>
		<session-timeout>5</session-timeout>
	</session-config>


	<listener>
		<listener-class>com.google.listener.UserCounterListener</listener-class>
	</listener>



</web-app>